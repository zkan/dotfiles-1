[user]
    name = Dien Nguyen
    email = dien@dienhn.com


[alias]
    b = branch
    ba = branch -a
    co = checkout

    a = add
    aa = add --all
    c = commit
    cm = commit -m
    cu = !git commit --date=\"$(date --utc +%Y-%m-%dT%H:%M:%S%z)\"
    d = diff
    dh = diff HEAD^ HEAD
    p = push

    s = status
    l = log
    lg = log --graph --decorate --oneline --all

    count = rev-list --count

    rl  = reflog
    rle = reflog expire --expire=now --expire-unreachable=now --all
    rlc = -c gc.reflogExpire=0 -c gc.reflogExpireUnreachable=0 -c gc.rerereresolved=0 -c gc.rerereunresolved=0 -c gc.pruneExpire=now gc

    rb = rebase -i --root
